# General shell command aliases and functions

# Navigation
alias ..="cd .."
alias ...="cd ../.."
alias ~="cd ~"
alias cddt="cd ~/dev-tools"

# Edit commonly used files/folders
alias ez="nvim ~/.zshrc"
alias eg="nvim ~/.gitconfig"
alias edt="nvim ~/dev-tools"

# Source on commonly used shell files
alias soz="source ~/.zshrc"
